# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                               controllers.Application.loginPage
#POST        /test                controllers.Application.test
POST        /register                       controllers.Application.register
POST        /upload                         controllers.Application.upload
GET         /images/:id                     controllers.Application.images(id: String)
GET         /tmpImages/:id                  controllers.Application.tempImages(id: String)
GET         /deleteImage/:id/:action        controllers.Application.deleteImages(id :String,action :Boolean)
POST        /login/:email/:password         controllers.Application.login(email :String,password :String)
GET         /cfp                            controllers.Application.index
GET         /talks                          controllers.Application.talksList
POST        /editTalk                       controllers.Application.editTalk

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file                   controllers.Assets.at(path="/public", file)
